<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="author" content="">
    @*<link rel="shortcut icon" href="img/demo/demo/logo-fav.png">*@
    <title>@ViewData["Title"] - myMEI</title>
    <link rel="stylesheet" type="text/css" href="~/lib/perfect-scrollbar/css/perfect-scrollbar.css" />
    <link rel="stylesheet" type="text/css" href="~/lib/material-design-icons/css/material-design-iconic-font.min.css" />
    <link rel="stylesheet" href="https://ej2.syncfusion.com/aspnetcore/styles/fabric.css" />
    <link rel="stylesheet" href="~/css/site.css" type="text/css" />
    <link rel="stylesheet" href="~/css/site-syncfusion-components.css" type="text/css" />

</head>
<body class="@ViewData["PageClass"]">
    <div class="be-wrapper">
        <nav class="navbar navbar-expand fixed-top be-top-header">
            <div class="container-fluid">
                <div class="be-navbar-header">
                    <a class="navbar-brand" href="#"></a>
                </div>
                <div class="page-title"><span>@*@ViewData["Title"]*@</span></div>
                <div class="be-right-navbar">
                    <vc:header-user-menu></vc:header-user-menu>
                    <ul class="nav navbar-nav float-right be-icons-nav">
                        <li class="nav-item"><a asp-page="/Clients/Index" class="nav-link">Clients</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Events/Index" role="link">Events</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Representatives/Index" role="link">Representatives</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Speakers/Index" role="link">Speakers</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Vendors/Index" role="link">Vendors</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Reports/Index" role="link">Reports</a></li>
                        <li class="nav-item"><a class="nav-link" asp-page="/Webinars/Index" role="link">Webinars</a></li>
                        @*<li class="nav-item dropdown"><a class="nav-link be-toggle-right-sidebar" href="#" role="button" aria-expanded="false"><span class="icon mdi mdi-settings"></span></a></li>*@
                        @* notifications *@
                        @*<partial name="_HeaderNotificationsPartial" />*@
                        @* app menu *@
                        @*<partial name="_HeaderAppsMenuPartial" />*@
                    </ul>
                </div>
            </div>
        </nav>
        <vc:left-sidebar currently-selected-link-text="@ViewData["CurrentlySelectedLinkTitle"]"></vc:left-sidebar>
        <div class="be-content">
            <div class="page-head">
                @RenderSection("MainLayoutPageHeader", required: false)
            </div>
            <div class="main-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
                
            </div>
        </div>

    </div>
    @RenderSection("MainBlazorScripts", required: false)
    @*<script src="~/_framework/blazor.server.js"></script>*@
    <script src="~/lib/jquery/dist/jquery.min.js" type="text/javascript"></script>
    <script src="~/lib/perfect-scrollbar/js/perfect-scrollbar.min.js" type="text/javascript"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" type="text/javascript"></script>
    <script src="~/lib/beagle/js/app.js" type="text/javascript"></script>
    <!-- Syncfusion Essential JS 2 Scripts -->
    <script src="https://cdn.syncfusion.com/ej2/17.2.49/dist/ej2.min.js "></script>
    <script src="https://cdn.syncfusion.com/ej2/17.2.49/dist/ejs.interop.min.js"></script>


    @*<script src="~/js/site.js" type="text/javascript"></script>*@

    <script type="text/javascript">
        $(document).ready(function () {
            //-initialize the javascript
            Beagle.init();
        });

    </script>

    @RenderSection("MainScriptSection", required: false)

    <!-- Syncfusion Essential JS 2 ScriptManager -->
    <ejs-scripts></ejs-scripts>


</body>
</html>